<h1>Listing city_maps</h1>

<table>
  <tr>
    <th>City</th>
    <th>Name</th>
    <th>Region</th>
    <th>Latitude</th>
    <th>Longitude</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @city_maps.each do |city_map| %>
  <tr>
    <td><%= city_map.city %></td>
    <td><%= city_map.name %></td>
    <td><%= city_map.region %></td>
    <td><%= city_map.latitude %></td>
    <td><%= city_map.longitude %></td>
    <td><%= link_to 'Show', city_map %></td>
    <td><%= link_to 'Edit', edit_city_map_path(city_map) %></td>
    <td><%= link_to 'Destroy', city_map, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New City map', new_city_map_path %>
